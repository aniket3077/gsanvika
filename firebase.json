{
  "firestore": {
    "database": "(default)",
    "location": "asia-south1",
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  },
  "functions": [
    {
      "source": "functions",
      "codebase": "default",
      "ignore": [
        "node_modules",
        ".git",
        "firebase-debug.log",
        "firebase-debug.*.log",
        "*.local"
      ],
      "predeploy": [
        "npm --prefix \"$RESOURCE_DIR\" run lint",
        "npm --prefix \"$RESOURCE_DIR\" run build"
      ]
    }
  ],
  "apphosting": [
    {
      "backendId": "e-com",
      "rootDir": "/",
      "ignore": [
        "node_modules",
        ".git",
        "firebase-debug.log",
        "firebase-debug.*.log",
        "functions",
        ".next"
      ],
      "env": {
        "NEXT_PUBLIC_FIREBASE_API_KEY": "AIzaSyA_IrpHC1dXG4UZVdfubnHtGTAj8Q1KiTU",
        "NEXT_PUBLIC_FIREBASE_AUTH_DOMAIN": "e-com-620fe.firebaseapp.com",
        "NEXT_PUBLIC_FIREBASE_PROJECT_ID": "e-com-620fe",
        "NEXT_PUBLIC_FIREBASE_STORAGE_BUCKET": "e-com-620fe.appspot.com",
        "NEXT_PUBLIC_FIREBASE_MESSAGING_SENDER_ID": "829977668997",
        "NEXT_PUBLIC_FIREBASE_APP_ID": "1:829977668997:web:c27a7fc8594bca51c7e8ec",
        "NEXT_PUBLIC_FIREBASE_MEASUREMENT_ID": "G-R8H21DPW4X"
      }
    }
  ],
  "storage": {
    "rules": "storage.rules"
  },
  "hosting": {
    "public": "out",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ],
    "headers": [
      {
        "source": "**/*.@(js|css)",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "public, max-age=31536000, immutable"
          }
        ]
      }
    ]
  },
  "database": {
    "rules": "database.rules.json"
  }
}
